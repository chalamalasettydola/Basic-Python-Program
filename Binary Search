def binary_search(arr, target):
    lb = 0
    ub = len(arr) - 1

    while lb <= ub:
        mid = (lb + ub) // 2  
        if arr[mid] == target:
            print("Element found at index", mid)
            return mid  
        elif arr[mid] < target:
            lb = mid + 1 
        else:
            ub = mid - 1 

    print("Element not found")
    return -1  

arr = [1, 3, 5, 7, 9, 11, 13, 15]
target = 7
binary_search(arr, target)

